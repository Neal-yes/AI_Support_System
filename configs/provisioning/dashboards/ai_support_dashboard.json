{
  "title": "AI Support - Export/Download",
  "uid": "ai-support-export-download",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {"from": "now-15m", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "7d"]
  },
  "templating": {
    "list": [
      {
        "type": "query",
        "name": "tenant",
        "label": "Tenant",
        "datasource": "Prometheus",
        "query": "label_values(tools_requests_total, tenant)",
        "includeAll": false,
        "refresh": 2,
        "multi": false
      },
      {
        "type": "query",
        "name": "collection",
        "label": "Collection",
        "datasource": "Prometheus",
        "query": "label_values(download_running, collection)",
        "includeAll": true,
        "refresh": 2,
        "multi": true
      },
      {
        "type": "query",
        "name": "status",
        "label": "HTTP Status (ask)",
        "datasource": "Prometheus",
        "query": "label_values(http_requests_total{path=\"/api/v1/ask\"}, status)",
        "includeAll": true,
        "refresh": 2,
        "multi": true
      },
      {
        "type": "query",
        "name": "has_match",
        "label": "RAG has_match",
        "datasource": "Prometheus",
        "query": "label_values(rag_matches_total, has_match)",
        "includeAll": true,
        "refresh": 2,
        "multi": true
      },
      {
        "type": "interval",
        "name": "interval",
        "label": "Rate Interval",
        "query": "5m,10m,15m,30m,1h",
        "auto": true,
        "auto_min": "1m"
      }
    ]
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "download_running by collection,gzip",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "download_running{tenant=~\"$tenant\",collection=~\"$collection\"}",
          "legendFormat": "{{collection}} gzip={{gzip}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "export_running by collection",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "export_running{tenant=~\"$tenant\",collection=~\"$collection\"}",
          "legendFormat": "{{collection}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 download_duration_seconds",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(download_duration_seconds_bucket{tenant=~\"$tenant\",collection=~\"$collection\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "export_status_total rate by status",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "sum by (status,collection) (rate(export_status_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{status}} {{collection}}",
          "refId": "A"
        }
      ]
    }
    ,
    {
      "type": "timeseries",
      "title": "tools_requests_total rate by tenant,type,name",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "sum by (tenant,tool_type,tool_name) (rate(tools_requests_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_errors_total rate by reason",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
      "targets": [
        {
          "expr": "sum by (reason,tenant,tool_type,tool_name) (rate(tools_errors_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{reason}} {{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 tools_request_latency_seconds",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(tools_request_latency_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_rate_limited_total rate",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "sum by (tenant,tool_type,tool_name) (rate(tools_rate_limited_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_circuit_open_total rate",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
      "targets": [
        {
          "expr": "sum by (tenant,tool_type,tool_name) (rate(tools_circuit_open_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_cache_hit_total rate",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
      "targets": [
        {
          "expr": "sum by (tenant,tool_type,tool_name) (rate(tools_cache_hit_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_retries_total rate",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 32},
      "targets": [
        {
          "expr": "sum by (tenant,tool_type,tool_name) (rate(tools_retries_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    }
    ,
    {
      "type": "stat",
      "title": "Total download_running",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 40},
      "targets": [
        {
          "expr": "sum(download_running{tenant=~\"$tenant\",collection=~\"$collection\"})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "tools_errors_total rate (all reasons)",
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 40},
      "targets": [
        {
          "expr": "sum(rate(tools_errors_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Export 成功率",
      "gridPos": {"h": 4, "w": 8, "x": 0, "y": 44},
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 1,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red" },
              { "value": 0.95, "color": "green" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "export_success_rate",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "工具错误占比",
      "gridPos": {"h": 4, "w": 8, "x": 8, "y": 44},
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "decimals": 1,
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green" },
              { "value": 0.05, "color": "red" }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {"calcs": ["lastNotNull"], "fields": "", "values": false},
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(tools_errors_total{tenant=~\"$tenant\"}[$__rate_interval])) / sum(rate(tools_requests_total{tenant=~\"$tenant\"}[$__rate_interval]))",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "tools_error_ratio_1m by tenant,type,name (recording rule)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 48},
      "targets": [
        {
          "expr": "tools_error_ratio_1m{tenant=~\"$tenant\"}",
          "legendFormat": "{{tenant}} {{tool_type}} {{tool_name}}",
          "refId": "A"
        }
      ]
    }
    ,
    {
      "type": "timeseries",
      "title": "ask http_requests_total rate by status",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 48},
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total{path=\"/api/v1/ask\",status=~\"$status\"}[$__rate_interval]))",
          "legendFormat": "status={{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 llm_generate_duration_seconds",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 56},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(llm_generate_duration_seconds_bucket[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "p95 rag_retrieval_duration_seconds",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 56},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(rag_retrieval_duration_seconds_bucket{collection=~\"$collection\"}[$__rate_interval])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "rag_matches_total rate by has_match",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 64},
      "targets": [
        {
          "expr": "sum by (has_match) (rate(rag_matches_total{collection=~\"$collection\",has_match=~\"$has_match\"}[$__rate_interval]))",
          "legendFormat": "has_match={{has_match}}",
          "refId": "A"
        }
      ]
    }
  ]
}
